        "$type": "color",
        "$value": "{colour.neutral.4}"
      },
      "5": {
        "$type": "color",
        "$value": "{colour.neutral.5}"
      },
      "6": {
        "$type": "color",
        "$value": "{colour.neutral.6}"
      },
      "skeleton": {
        "$type": "color",
        "$value": "{skeleton.dark}"
      }
    },
    "foreground": {
      "blockquote": {
        "$type": "color",
        "$value": "{colour.neutral.100}"
