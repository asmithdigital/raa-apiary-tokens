    "light": {
      "$type": "color",
      "$value": "{colour.neutral.light}"
    }
  },
  "spacing": {
    "2xl": {
      "$type": "number",
      "$value": 96
    },
    "lg": {
      "$type": "number",
      "$value": 48
    },
    "md": {
      "$type": "number",
      "$value": 24
