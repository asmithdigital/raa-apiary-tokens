      "$type": "color",
      "$value": "#441fcc"
    },
    "brand": {
      "active": {
        "$type": "color",
        "$value": "{layer.background.brand-contrast}"
      },
      "default": {
        "$type": "color",
        "$value": "{colour.yellow.100}"
      },
      "focus": {
        "$type": "color",
        "$value": "{layer.background.default}"
      },
      "hover": {
        "$type": "color",
