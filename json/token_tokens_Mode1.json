      "invert": {
        "$type": "color",
        "$value": "{colour.neutral.0}"
      }
    },
    "icon": {
      "brand": {
        "$type": "color",
        "$value": "{colour.neutral.100}"
      },
      "default": {
        "$type": "color",
        "$value": "{colour.neutral.100}"
      },
      "invert": {
        "$type": "color",
        "$value": "{colour.neutral.0}"
      }
    }
