    "link": {
      "$type": "color",
      "$value": "#00349f"
    },
    "neutral": {
      "active": {
        "$type": "color",
        "$value": "{colour.neutral.10}"
      },
      "focus": {
        "$type": "color",
        "$value": "{layer.background.default}"
      },
      "hover": {
        "$type": "color",
        "$value": "{colour.neutral.5}"
      },
      "secondary": {
        "$type": "color",
        "$value": "{colour.neutral.0}"
      }
    },
    "selected": {
      "default": {
        "$type": "color",
        "$value": "{colour.green-dark.100}"
      }
    },
    "success": {
      "background": {
        "$type": "color",
        "$value": "{colour.green-dark.10}"
      },
      "border": {
        "$type": "color",
        "$value": "{colour.green-dark.100}"
      },
      "foreground": {
        "$type": "color",
        "$value": "{colour.green-dark.100}"
      },
      "icon": {
        "$type": "color",
        "$value": "{colour.green-dark.100}"
      }
    },
    "visited": {
      "$type": "color",
      "$value": "#441fcc"
    }
